{
  "name": "simple-asset-monitor",
  "version": "1.0.0",
  "description": "Simple IT Asset Monitoring System",
  "main": "server/app.js",
  "scripts": {
    "start": "node server/app.js",
    "dev": "nodemon server/app.js",
    "install-all": "npm install && cd agent && pip install -r requirements.txt && cd ../ml-service && pip install -r requirements.txt",
    "start-ml": "python start_ml_service.py",
    "start-all": "concurrently \"npm start\" \"npm run start-ml\""
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "axios": "^1.4.0",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "mongoose": "^7.0.3",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "nodemon": "^2.0.22"
  },
  "keywords": [
    "asset",
    "monitoring",
    "IT",
    "hardware",
    "software"
  ],
